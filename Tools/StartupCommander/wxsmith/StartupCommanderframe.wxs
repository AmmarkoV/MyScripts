<?xml version="1.0" encoding="utf-8" ?>
<wxsmith>
	<object class="wxFrame" name="StartupCommanderFrame">
		<title>StartupCommander</title>
		<size>553,450</size>
		<object class="wxButton" name="ID_BUTTON1" variable="ButtonStartup" member="yes">
			<label>Use Startup File</label>
			<pos>88,104</pos>
			<size>368,88</size>
			<handler function="OnButtonStartupClick" entry="EVT_BUTTON" />
		</object>
		<object class="wxButton" name="ID_BUTTON2" variable="ButtonExit" member="yes">
			<label>Exit</label>
			<pos>88,264</pos>
			<size>368,32</size>
			<handler function="OnButtonExitClick" entry="EVT_BUTTON" />
		</object>
		<object class="wxGauge" name="ID_GAUGE1" variable="GaugeTimeout" member="yes">
			<pos>88,192</pos>
			<size>368,28</size>
		</object>
		<object class="wxMenuBar" variable="MenuBar1" member="no">
			<object class="wxMenu" variable="Menu1" member="no">
				<label>&amp;File</label>
				<object class="wxMenuItem" name="idMenuQuit" variable="MenuItem1" member="no">
					<label>Quit</label>
					<accel>Alt-F4</accel>
					<help>Quit the application</help>
					<handler function="OnQuit" entry="EVT_MENU" />
				</object>
			</object>
			<object class="wxMenu" variable="Menu2" member="no">
				<label>Help</label>
				<object class="wxMenuItem" name="idMenuAbout" variable="MenuItem2" member="no">
					<label>About</label>
					<accel>F1</accel>
					<help>Show info about this application</help>
					<handler function="OnAbout" entry="EVT_MENU" />
				</object>
			</object>
		</object>
		<object class="wxStatusBar" name="ID_STATUSBAR1" variable="StatusBar1" member="yes">
			<fields>1</fields>
			<widths>-1</widths>
			<styles>wxSB_NORMAL</styles>
		</object>
		<object class="wxTimer" name="ID_TIMER1" variable="Timer1" member="yes">
			<interval>100</interval>
			<handler function="OnTimer1Trigger" entry="EVT_TIMER" />
		</object>
	</object>
</wxsmith>
